{
  "name": "bpmn-analyzer-js",
  "version": "0.0.1",
  "description": "bpmn-js including analysis after each save",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "all": "run-s lint pCheck test:run build",
    "build": "vite build",
    "lint": "eslint .",
    "start": "vite",
    "dev": "vite",
    "preview": "vite preview",
    "build:gh-pages": "rimraf ./docs && vite build --mode ghPages",
    "build:rust": "rimraf ../rust_bpmn_analyzer/webserver/public && vite build --mode rust",
    "pCheck": "prettier . --check",
    "pWrite": "prettier . --write",
    "test": "vitest",
    "test:run": "vitest run",
    "test:e2e": "npx playwright test"
  },
  "author": {
    "name": "Tim KrÃ¤uter",
    "url": "https://timkraeuter.com/"
  },
  "files": [
    "src",
    "index.js"
  ],
  "engines": {
    "node": ">= 18"
  },
  "sideEffects": [
    "*.css"
  ],
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.3",
    "vite": "^7.3.1",
    "vite-plugin-static-copy": "^3.2.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "bpmn-js": "^18.12.0",
    "diagram-js": "^15.9.1",
    "min-dash": "^5.0.0",
    "min-dom": "^5.3.0",
    "randomcolor": "^0.6.2",
    "tiny-svg": "^4.1.4"
  }
}
