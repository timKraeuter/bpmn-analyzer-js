{
  "name": "bpmn-analyzer-js",
  "version": "0.0.1",
  "description": "bpmn-js including analysis after each save",
  "main": "app/index.js",
  "scripts": {
    "all": "run-s lint pCheck build",
    "build": "webpack",
    "lint": "eslint .",
    "start": "run-s dev",
    "dev": "run-p build:watch serve",
    "build:watch": "webpack -w",
    "serve": "serve public",
    "build:gh-pages": "rimraf ./docs && webpack --env ghPages",
    "build:rust": "rimraf ../rust_bpmn_analyzer/webserver/public && webpack --env rust",
    "pCheck": "prettier . --check",
    "pWrite": "prettier . --write"
  },
  "author": {
    "name": "Tim KrÃ¤uter",
    "url": "https://timkraeuter.com/"
  },
  "files": [
    "src",
    "index.js"
  ],
  "engines": {
    "node": ">= 16"
  },
  "sideEffects": [
    "*.css"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.3",
    "raw-loader": "^4.0.2",
    "rimraf": "^6.1.0",
    "serve": "^14.2.3",
    "style-loader": "^4.0.0",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  },
