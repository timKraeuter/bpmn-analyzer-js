    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-bpmn-io": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "raw-loader": "^4.0.2",
    "rimraf": "^6.0.1",
